{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputLogs": "errors-only",
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "devs": {
      "persistent": true,
      "cache": false
    },
    "sync": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  }
}