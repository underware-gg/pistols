{
  "name": "pistols-client",
  "version": "0.12.5",
  "description": "Pistols at 10 Blocks JS client",
  "homepage": "https://github.com/underware-gg/pistols",
  "author": "Underware",
  "license": "CC0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devs": "vite -c vite_https.config",
    "build": "npx tsc -p tsconfig.json && vite build",
    "check-types": "npx tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "create-constants": "npx @underware_gg/pistols-sdk generate-constants --src:\"../dojo/src\" --out:\"$npm_config_sdk/generated/constants.ts\""
  },
  "keywords": [
    "pistols at 10 blocks",
    "underware",
    "dojo",
    "typescript",
    "client"
  ],
  "dependencies": {
    "@apollo/client": "catalog:",
    "@cartridge/connector": "catalog:",
    "@cartridge/controller": "catalog:",
    "@dojoengine/core": "catalog:",
    "@dojoengine/sdk": "catalog:",
    "@dojoengine/torii-client": "catalog:",
    "@dojoengine/utils": "catalog:",
    "@starknet-react/chains": "catalog:",
    "@starknet-react/core": "catalog:",
    "@tweenjs/tween.js": "^21.0.0",
    "@underware_gg/pistols-sdk": "workspace:*",
    "event-emitter": "^0.3.5",
    "get-starknet-core": "catalog:",
    "immer": "catalog:",
    "lil-gui": "^0.19.2",
    "react": "catalog:",
    "react-circular-progressbar": "^2.1.0",
    "react-cookie": "^6.1.1",
    "react-dom": "catalog:",
    "react-router": "^7.1.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^3.0.0-beta.2",
    "starknet": "catalog:",
    "starknetid.js": "catalog:",
    "three": "^0.157.0",
    "type-fest": "^2.14.0",
    "use-local-storage-state": "^19.1.0",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.30",
    "@types/react-dom": "^18.2.14",
    "@types/three": "^0.157.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "sass": "^1.83.0",
    "typescript": "catalog:",
    "vite": "^6.0.5",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "3.0.0-beta.2"
  },
  "engines": {
    "node": ">=22"
  }
}
